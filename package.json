{
  "name": "nomina-sanfrancisco",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seeds:generate": "sequelize-cli seed:generate --name",
    "seeds:run": "sequelize-cli db:seed:all",
    "seeds:revert": "sequelize-cli db:seed:undo",
    "seeds:delete": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "@react-pdf/renderer": "^3.0.1",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "chart.js": "^4.0.1",
    "dotenv": "^16.0.3",
    "install": "^0.13.0",
    "joi": "^17.6.3",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0",
    "moment": "^2.29.4",
    "next": "12.3.1",
    "next-connect": "^0.13.0",
    "nodemailer": "^6.8.0",
    "npm": "^9.1.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "react": "18.2.0",
    "react-chartjs-2": "^5.0.1",
    "react-data-table-component": "^7.5.3",
    "react-dom": "18.2.0",
    "react-google-maps": "^9.4.5",
    "sequelize": "^6.25.0",
    "sequelize-cli": "^6.5.1",
    "sequelize-import-export": "^1.0.1",
    "sweetalert2": "^11.6.4"
  },
  "devDependencies": {
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1"
  }
}
